<data>

<record id="hr_leave_allocation_pre_filter" model="ir.filters">
    <field name="name">Leave Allocation Pre-Filter</field>
    <field name="model_id">hr.leave_allocation</field>
    <field name="domain">[('num_of_days_available','>',0)]</field>
    <field name="active" eval="1" />
    <field name="user_id" eval="0" />
</record>

<record id="hr_leave_allocation_filter" model="ir.filters">
    <field name="name">Leave Allocation Filter</field>
    <field name="model_id">hr.leave_allocation</field>
    <field name="domain">[('num_of_days_available','=',0)]</field>
    <field name="active" eval="1" />
    <field name="user_id" eval="0" />
</record>




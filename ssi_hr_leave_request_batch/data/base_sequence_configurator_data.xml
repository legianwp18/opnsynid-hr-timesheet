<openerp>
<data>
    <record id="base_sequence_leave_request_batch" model="base.sequence_configurator">
        <field
                name="model_id"
                ref="ssi_hr_leave_request_batch.model_hr_leave_request_batch"
            />
        <field
                name="sequence_field_id"
                search="[('model_id.model','=','hr.leave_request_batch'),('name','=','name')]"
            />
        <field
                name="fallback_sequence_id"
                ref="ssi_hr_leave_request_batch.sequence_leave_request_batch"
            />
    </record>

    <record
            id="base_sequence_leave_request_batch_line"
            model="base.sequence_configurator_line"
        >
        <field name="generator_id" ref="base_sequence_leave_request_batch" />
        <field name="sequence" eval="1" />
        <field
                name="sequence_computation_code"
            >result = document.type_id.sequence_batch_id</field>
    </record>
</data>
</openerp>
